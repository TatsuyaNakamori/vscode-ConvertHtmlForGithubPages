{
  "name": "htmlgithubpages",
  "displayName": "htmlgithubpages",
  "description": "%ext.description%",
  "version": "0.0.1",
  "publisher": "TatsuyaNakamori",
  "engines": {
    "vscode": "^1.46.0"
  },
  "galleryBanner": {
    "color": "#C0C0C8",
    "theme": "dark"
  },
  "icon": "resources/icon/extension_main.png",
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "preview": true,
  "keywords": [
      "github",
      "pages",
      "html",
      "css",
      "jekyll"
  ],
  "homepage": "https://github.com/TatsuyaNakamori/vscode-ConvertHtmlForGithubPages",
  "repository": {
      "type": "git",
      "url": "https://github.com/TatsuyaNakamori/vscode-ConvertHtmlForGithubPages.git"
  },
  "bugs": {
      "url": "https://github.com/TatsuyaNakamori/vscode-ConvertHtmlForGithubPages/issues"
  },
  "activationEvents": [
    "onCommand:workbench.action.tasks.runTask",
    "onCommand:htmlgithubpages.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "htmlgithubpages.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": [
      {
        "title": "GitHub Pages",
        "properties": {
          "ConvertHtmlForGithubPages.source.fromTo": {
            "type": "object",
            "properties": {
              "From": {
                "type": "string"
              },
              "To": {
                "type": "string"
              }
            },
            "default": {
              "From": "./",
              "To": "./docs"
            },
            "uniqueItems": true,
            "markdownDescription": "%conf.source.fromTo%"
          }
        }
      }
    ],
    "taskDefinitions": [
      {
          "type": "to github pages",
          "required": [
              "command",
              "label"
          ],
          "properties": {
              "command": {
                  "type": "string",
                  "description": "test- test"
              },
              "label": {
                  "type": "string"
              }
          }
      }
  ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  }
}
